    "test": "karmatic 'tests/*.test.ts'",
    "prepublishOnly": "node prepublish.js"
  },
  "dependencies": {
    "redaxios": "^0.4.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "karmatic": "^2.1.0",
    "microbundle": "^0.13.0",
    "prettier": "^2.2.1",
    "prettier-config-rschristian": "^0.1.0",
    "ts-loader": "^8.0.4",
    "typescript": "^4.2.4",
    "webpack": "^4.44.2"
  },
  "prettier": "prettier-config-rschristian"
}
